# Install Script

build-install:
	MKDIR_NOCLEAN %SPXLIB/encryption
	MKDIR_NOCLEAN %SPXINC/encryption
	COPY_FILE ./md2.h %SPXINC/encryption
	COPY_FILE ./libencryption.a %SPXLIB/encryption
	COPY_FILE ./libencryption.so.%PKG_MAJOR.%PKG_MINOR.%PKG_AUX %SPXLIB/encryption
	ADD_LIBRARY_LINKS %SPXLIB/encryption libencryption.so.%PKG_MAJOR.%PKG_MINOR.%PKG_AUX


debug-install:
