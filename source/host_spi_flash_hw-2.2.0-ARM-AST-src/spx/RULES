#Extract Source in %SOURCE
extract_source:

#Build Source in %BUILD
build_source:
	CWD %BUILD/%PACKAGE/data
	INVOKE_MAKE

#Clean Source
clean_source:
	CWD %BUILD/%PACKAGE/data
	INVOKE_MAKE clean

#-----------------Mandatory Rules for every binary Package ----------------------------
#Rules to create host_spi_flash_hw package
build_package-host_spi_flash_hw:
	PACK_SPX ./ %BUILD/%PACKAGE/data/host_spi_flash_hw.ko
