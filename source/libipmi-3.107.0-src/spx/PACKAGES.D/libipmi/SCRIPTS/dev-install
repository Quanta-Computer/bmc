# Install Script

build-install:
		COPY_DIR		./ %IMAGE_TREE/usr/local/lib/
		ADD_LIBRARY_LINKS %IMAGE_TREE/usr/local/lib libipmi.so.%PKG_MAJOR.%PKG_MINOR.%PKG_AUX
		EXECUTE if [ "%CONFIG_SPX_FEATURE_DISABLE_LIBIPMI_IPMB_INTERFACE" == "YES" ]; then  echo "CONFIG_SPX_FEATURE_DISABLE_LIBIPMI_IPMB_INTERFACE=YES" >> %IMAGE_TREE/etc/core_features; fi
debug-install:
