#Extract Source in %SOURCE
extract_source:

#Build Source in %BUILD
build_source:
	CWD %BUILD/%PACKAGE/data
	INVOKE_MAKE

#Clean Source
clean_source:
	CWD %BUILD/%PACKAGE/data
	INVOKE_MAKE clean

#-----------------Mandatory Rules for every binary Package ----------------------------
# Rules to create access_reg_mod package
build_package-access_reg_mod:
		PACK_SPX  ./   %BUILD/%PACKAGE/data/access_reg.ko

# Rules to create access_reg_mod_driver_dev package
build_package-access_reg_mod_driver_dev:
		MKDIR_CLEAN %SPXINC/access_reg
		COPY_FILE %BUILD/%PACKAGE/data/access_reg.h %SPXINC/access_reg
