#Extract Source	in %SOURCE
extract_source:

#Build Source in %BUILD
build_source:
		CWD %BUILD/%PACKAGE/data
		INVOKE_MAKE

#Clean Source
clean_source:
	CWD %BUILD/%PACKAGE/data
		INVOKE_MAKE clean
		
#--------------------- Mandatory Rules for every binary package -------------------------
# Rules to create libipmiuds package
build_package-libipmiuds:
		PACK_SPX  ./   %BUILD/%PACKAGE/data/libipmilocal.so.%PKG_MAJOR.%PKG_MINOR.%PKG_AUX
		

