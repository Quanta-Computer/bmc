# Install Script

build-install:
                COPY_DIR                ./ %IMAGE_TREE/usr/local/lib/
                ADD_LIBRARY_LINKS %IMAGE_TREE/usr/local/lib libsnmp_hostname_mib.so.%PKG_MAJOR.%PKG_MINOR.%PKG_AUX
				EXECUTE if [ -f %IMAGE_TREE/conf/snmp_users.conf ]; then grep -c libsnmp_hostname_mib %IMAGE_TREE/conf/snmp_users.conf || echo "dlmod libsnmp_hostname_mib /usr/local/lib/libsnmp_hostname_mib.so" >> %IMAGE_TREE/conf/snmp_users.conf; fi
                EXECUTE if [ -f %IMAGE_TREE/etc/defconfig/snmp_users.conf ]; then grep -c libsnmp_hostname_mib %IMAGE_TREE/etc/defconfig/snmp_users.conf || echo "dlmod libsnmp_hostname_mib /usr/local/lib/libsnmp_hostname_mib.so" >> %IMAGE_TREE/etc/defconfig/snmp_users.conf; fi
                
debug-install:
